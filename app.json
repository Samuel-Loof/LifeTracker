{
  "expo": {
    "scheme": "shelfieapp",
    "name": "LifeTrack3r",
    "slug": "LifeTrack3r",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.discosam.LifeTrack3r",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "LifeTrack3r needs access to your camera to scan food barcodes and track your nutrition.",
        "NSPhotoLibraryUsageDescription": "LifeTrack3r needs access to your photo library to save and share your progress."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "POST_NOTIFICATIONS",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.discosam.LifeTrack3r",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow LifeTrack3r to access your camera to scan food barcodes.",
          "microphonePermission": false
        }
      ],
      "expo-font",
      "expo-notifications"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8e7a1398-81a2-4f6a-8731-cf14dbc77451"
      }
    }
  }
}
