node_modules/

# Build artifacts
.expo/
dist/
web-build/
*.tsbuildinfo

# Native folders (managed by Expo Prebuild)
android/
ios/
.kotlin/

# Git
.git/
.gitignore

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db
desktop.ini

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Environment files
.env*.local
.env

# Certificates and keys
*.pem
*.p8
*.p12
*.key
*.jks
*.mobileprovision
*.orig.*

# Temporary files
*.tmp
*.temp
.cache/

